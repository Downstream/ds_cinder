<settings>
	<text name="logger:level" value="none" />
	<text name="logger:module" value="none" />
	<!-- TEMPLATIZED PROJECT GENERATION -->
	<!--
		Note: in order to generate projects from different templates,
		your specified template needs to have these files:
		* [template name]_app.cpp
		* [template name]_app.h (optional)
		* [template name].vcxproj
		Also you should supply app's base classname, namespace, and headguard
		classname is inside [template name]_app.cpp
		namespace is used across the entire template
		headguard will be replaced in all template headers
	-->
	<text name="template:path" value="%ds_platform_086%\\example\\full_starter" />
	<text name="template:name" value="full_starter" />
	<text name="template:classname" value="FullStarterApp" />
	<text name="template:namespace" value="fullstarter" />
	<text name="template:headguard" value="FULLSTARTER" />
</settings>
